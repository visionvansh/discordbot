{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/send-message.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/send-message",
      "dest": "api/send-message.js"
    },
    {
      "src": "/(.*)",
      "dest": "public/index.html"
    }
  ]
}